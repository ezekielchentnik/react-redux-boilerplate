{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate built with react/redux to spin up fantastic micro apps",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && NODE_ENV=development webpack --config webpack.config.prod.js",
    "start": "node devServer.js",
    "lint": "eslint src",
    "test": "mocha --compilers js:babel-core/register --recursive --require ./test/setup.js",
    "test:watch": "npm test -- --watch"
  },

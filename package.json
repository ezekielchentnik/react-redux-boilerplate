    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform",
    "redux",
    "thunk",
    "middleware",
    "redux-middleware",
    "flux",
    "mocha"
  ],
  "author": "Ezekiel Chentnik <me@ezekielchentnik.com> (http://github.com/ezekielchentnik)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ezekielchentnik/react-redux-boilerplate/issues"
